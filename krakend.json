{
  "version": 2,
  "extra_config": {
    "github_com/devopsfaith/krakend-gologging": {
      "level": "WARNING",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true,
      "format": "logstash"
    },
    "github_com/devopsfaith/krakend-logstash": {
      "enabled": false
    }
  },
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "octopus_gateway",
  "endpoints": [
    {
      "endpoint": "/token",
      "method": "POST",
      "extra_config": {
        "github.com/devopsfaith/krakend-jose/signer": {
          "alg": "RS256",
          "jwk-url": "http://localhost:9810/.well-known/jwks.json",
          "disable_jwk_security": true,
          "keys-to-sign": [
            "accessToken"
          ],
          "kid": "7b4b0bf8-f992-4f80-a5f5-50a55ec3917e"
        }
      },
      "output_encoding": "json",
      "concurrent_calls": 1,
      "backend": [
        {
          "url_pattern": "/token-issuer",
          "encoding": "json",
          "sd": "static",
          "extra_config": {},
          "method": "POST",
          "disable_host_sanitize": true,
          "host": [
            "localhost:9810",
            "http://localhost:9810"
          ]
        }
      ]
    }
  ]
}